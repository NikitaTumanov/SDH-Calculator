<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<VBox fx:id="prescriptionPane" alignment="CENTER" spacing="10.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.sdhcalculator.HelloController">

    <Label text="Предполагаемая давность СДГ" styleClass="label-bottom"/>
    <Button text="≤ 48 ч" onAction="#onLessFortyEightButtonClick"/>
    <Button text="&gt; 48 ч" onAction="#onOverFortyEightButtonClick"/>

    <HBox spacing="20" alignment="CENTER">
        <Button text="Назад" onAction="#onBackButtonClick"/>
        <Button text="В начало" onAction="#onHomeButtonClick"/>
    </HBox>
</VBox>
